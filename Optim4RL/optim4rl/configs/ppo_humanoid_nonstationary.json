{
  "env": [{
    "name": ["humanoid"],
    "train_steps": [1e9],
    "episode_length": [1000],
    "action_repeat": [1],
    "reward_scaling": [0.1],
    "num_envs": [2048],
    "num_evals": [20],
    "normalize_obs": [true],
    "non_stationary": [true],
    "task_type": ["humanoid"]
  }],
  "agent": [{
    "name": ["PPO"],
    "gae_lambda": [0.95],
    "rollout_steps": [10],
    "num_minibatches": [32],
    "clipping_epsilon": [0.3],
    "update_epochs": [8],
    "entropy_weight": [1e-3]
  }],
  "optim": [{
    "name": ["Adam"],
    "kwargs": [{"learning_rate": [3e-4], "grad_clip": [1]}]
  }],
  "batch_size": [1024],
  "discount": [0.97],
  "max_devices_per_host": [-1],
  "seed": [1],
  "generate_random_seed": [true]
}
