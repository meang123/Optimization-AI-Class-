{
  "env": [{
    "name": ["humanoid"],
    "train_steps": [1e9],
    "episode_length": [1000],
    "action_repeat": [1],
    "reward_scaling": [0.1],
    "num_envs": [2048],
    "normalize_obs": [true],
    "non_stationary": [true],
    "task_type": ["humanoid"]
  }],
  "agent": [{
    "name": ["MetaPPO"],
    "inner_updates": [2],
    "gae_lambda": [0.95],
    "rollout_steps": [5],
    "num_minibatches": [4],
    "clipping_epsilon": [0.3],
    "update_epochs": [4],
    "entropy_weight": [1e-3],
    "reset_interval": [128]
  }],
  "agent_optim": [{
    "name": ["Optim4RL"],
    "kwargs": [{
      "param_load_path": [""],
      "learning_rate": [3e-4],
      "grad_clip": [1]
    }]
  }],
  "meta_optim": [{
    "name": ["Adam"],
    "kwargs": [{
      "learning_rate": [3e-4],
      "grad_clip": [1.0]
    }]
  }],
  "display_interval": [10],
  "save_param": [100000],
  "batch_size": [512],
  "discount": [0.97],
  "max_devices_per_host": [-1],
  "seed": [1],
  "generate_random_seed": [true]
}
