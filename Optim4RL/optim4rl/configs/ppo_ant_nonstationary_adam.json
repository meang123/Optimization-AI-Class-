{
  "env": [{
    "name": ["ant"],
    "train_steps": [1e9],
    "episode_length": [1000],
    "action_repeat": [1],
    "reward_scaling": [10],
    "num_envs": [4096],
    "num_evals": [20],
    "normalize_obs": [true],
    "non_stationary": [true],
    "task_type": ["ant"]
  }],
  "agent": [{
    "name": ["PPO"],
    "gae_lambda": [0.95],
    "rollout_steps": [5],
    "num_minibatches": [32],
    "clipping_epsilon": [0.3],
    "update_epochs": [4],
    "entropy_weight": [1e-2]
  }],
  "optim": [{
    "name": ["Adam"],
    "kwargs": [{"learning_rate": [3e-4],"b1": [0.997], "b2": [0.997],"grad_clip": [1]}]
  }],
  "batch_size": [2048],
  "discount": [0.97],
  "max_devices_per_host": [-1],
  "seed": [1],
  "generate_random_seed": [true],
  "save_policy_interval": [5]
}
