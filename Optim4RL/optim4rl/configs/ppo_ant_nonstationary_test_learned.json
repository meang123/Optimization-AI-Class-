{
  "env": [{
    "name": ["ant"],
    "train_steps": [1e9],
    "episode_length": [1000],
    "action_repeat": [1],
    "reward_scaling": [10],
    "num_envs": [2048],
    "num_evals": [20],
    "normalize_obs": [true],
    "non_stationary": [true],
    "task_type": ["ant"]
  }],
  "agent": [{
    "name": ["PPO"],
    "gae_lambda": [0.95],
    "rollout_steps": [3],
    "num_minibatches": [4],
    "clipping_epsilon": [0.3],
    "update_epochs": [4],
    "l2_weight": [0.0],
    "entropy_weight": [1e-2]
  }],
  "optim": [{
    "name": ["Optim4RL"],
    "kwargs": [{
      "param_load_path": ["./logs/meta_ppo_ant_nonstationary_learned_lowmem/1/param.pickle"],
      "learning_rate": [3e-4],
      "grad_clip": [1]
    }]
  }],
  "batch_size": [512],
  "discount": [0.97],
  "max_devices_per_host": [-1],
  "seed": [1],
  "generate_random_seed": [true],
  "save_policy_interval": [5]
}
